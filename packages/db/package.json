{
  "name": "db",
  "license": "AGPL-3.0-or-later",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "prisma generate --schema=./postgresql/schema.prisma && prisma generate --schema=./mysql/schema.prisma",
    "db:push": "prisma db push --skip-generate --schema=./postgresql/schema.prisma",
    "db:migrate": "prisma migrate deploy --schema=./postgresql/schema.prisma",
    "create:migration": "prisma migrate dev --schema=./postgresql/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "4.9.0"
  },
  "devDependencies": {
    "dotenv-cli": "7.0.0",
    "prisma": "4.9.0",
    "tsconfig": "workspace:*",
    "typescript": "4.9.4"
  }
}
